func calculate_line_wins(wins):
	if(wins == null): return 0.0;
	var n : float = 0.0;

	for win in wins: 
		if(win[\"index\"].findn(\"freespin\")>-1): continue;
		if(!win.has(\"winline\")): n+=float(win[\"win\"]); #winline 0
		elif(int(win[\"winline\"]) > -1): n+=float(win[\"win\"]);

	return n;

